    "@mw-components/koid": "^9.2.0",
    "@typegoose/typegoose": "^10.0.0",
    "@waiting/egg-jwt": "^6.2.0",
    "@waiting/shared-core": "^14.4.2",
    "@waiting/shared-types": "^14.4.2",
