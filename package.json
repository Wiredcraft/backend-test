{
  "name": "user-service",
  "version": "1.0.0",
  "description": "general user management service",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "build": "babel src -d dist",
    "start": "node index.js",
    "start:dev": "nodemon",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.2",
    "@nestjs/core": "^7.0.2",
    "@nestjs/platform-express": "^7.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.7",
    "@nestjs/testing": "^7.0.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "test",
    "testRegex": ".spec.js$",
    "coverageDirectory": "../coverage"
  }
}
