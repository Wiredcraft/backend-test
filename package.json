  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@midwayjs/cli": "^1.2.83",
    "@midwayjs/cli-plugin-faas": "^1.2.77",
    "@midwayjs/egg-ts-helper": "^1.2.1",
    "@midwayjs/fcli-plugin-fc": "^1.2.77",
