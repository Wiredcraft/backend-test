{
  "name": "test-backend",
  "version": "1.0.0",
  "description": "WiredCraft Backend Engineer Interview Test",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t test-backend .",
    "docker:run": "docker run -p 3000:3000 -d test-backend",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "oseifrimpong",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^5.0.0",
    "@loopback/authentication-jwt": "^0.5.0",
    "@loopback/boot": "^2.4.1",
    "@loopback/core": "^2.9.3",
    "@loopback/extension-health": "^0.4.11",
    "@loopback/extension-logging": "^0.3.11",
    "@loopback/repository": "^2.11.0",
    "@loopback/rest": "^6.0.0",
    "@loopback/rest-explorer": "^2.2.8",
    "@loopback/service-proxy": "^2.3.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.8.0",
    "isemail": "^3.2.0",
    "loopback-connector-mongodb": "^5.2.3",
    "loopback4-ratelimiter": "^1.0.0",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "superagent": "^6.0.0",
    "tsc-watch": "^4.2.9",
    "tslib": "^2.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/eslint-config": "^9.0.0",
    "@loopback/testlab": "^3.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^10.17.28",
    "eslint": "^7.6.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.7"
  }
}
