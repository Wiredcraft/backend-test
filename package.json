{
  "name": "test-backend",
  "version": "1.0.0",
  "description": "demo for Wiredcraft backend test",
  "main": "index.js",
  "scripts": {
    "start": "debug=test-backend:* NODE_ENV=development nodemon server.js",
    "test": "NODE_ENV=test mocha --recursive --exit",
    "cover": "NODE_ENV=test nyc --reporter=cobertura mocha  --recursive --exit",
    "prettier": "prettier --write 'server/**/*.js'",
    "precommit": "lint-staged",
    "lint": "eslint --fix --ext .js server",
    "doc": "cd document && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wiredcraft/test-backend.git"
  },
  "keywords": [
    "demo"
  ],
  "engines": {
    "node": ">=8.10.0"
  },
  "author": "shisang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Wiredcraft/test-backend/issues"
  },
  "homepage": "https://github.com/Wiredcraft/test-backend#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.11",
    "pbkdf2": "^3.0.17",
    "pwd": "^1.1.0",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-koa": "^2.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "json-refs": "^3.0.13",
    "lint-staged": "^8.0.4",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pm2": "^4.1.2",
    "prettier": "^1.19.1",
    "should": "^13.2.3",
    "sinon": "^7.5.0",
    "swagger-ui-dist": "^3.24.2",
    "yaml-js": "^0.2.3"
  },
  "lint-staged": {
    "server/**/*.js": [
      "eslint --ext .js --fix",
      "npm run prettier",
      "git add"
    ]
  }
}
