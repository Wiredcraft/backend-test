{
  "apps": {
      "name": "Wired",

      "script": "./build/server.js",
      "watch": true, 
      "ignore_watch": [
          "node_modules",
          "logs"
        ],
      "exec_mode": "cluster",
      "instances": 1,
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
  }
}